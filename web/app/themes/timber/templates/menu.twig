<ul class="navbar-nav ml-auto">
{% for item in test_menu.items %}
	<li class="nav-item
		{{ item.classes | join(' ') }}
		{% if item.children %} dropdown {% endif %}">
		<a class="nav-link {% if item.children %} dropdown-toggle {% endif %}"
			href="{{ item.link }}"
			{% if item.children %}
			data-toggle="dropdown"
			target="{{ item.target }}"
			aria-haspopup="true"
			aria-expanded="false"
			{% endif %}>
			{{ item.title }}
		</a>
		{% if item.children %}
			<div class="dropdown-menu">
				{% for child in item.children %}
					<a href="{{ child.link }}" class="dropdown-item">
						{{ child.title }}
					</a>
				{% endfor %}
			</div>
		{% endif %}
	</li>
{% endfor %}
</ul>